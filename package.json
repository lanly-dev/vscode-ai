{
  "name": "ai",
  "displayName": "ai",
  "description": "Something related to AI",
  "version": "0.0.1",
  "publisher": "lanly-dev",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hy.loginWebsite",
        "title": "Hunyuan 3D Login",
        "icon": "$(link)",
        "category": "AI"
      },
      {
        "command": "hy.loggingIn",
        "title": "Hunyuan 3D Logging In...",
        "icon": "$(key)",
        "category": "AI"
      },
      {
        "command": "hy.getConfig",
        "title": "Get Hunyuan Config",
        "icon": "$(settings)",
        "category": "AI"
      },
      {
        "command": "hy.getUserInfo",
        "title": "Get User Info",
        "icon": "$(user)",
        "category": "AI"
      },
      {
        "command": "hy.getQuotaInfo",
        "title": "Get Quota Info",
        "icon": "$(gear)",
        "category": "AI"
      },
      {
        "command": "hy.generate3DModel",
        "title": "Generate 3D Model",
        "icon": "$(rocket)",
        "category": "AI"
      },
      {
        "command": "hy.getCreationDetails",
        "title": "Get Creation Details",
        "icon": "$(file-code)",
        "category": "AI"
      },
      {
        "command": "hy.listCreations",
        "title": "List Creations",
        "icon": "$(list-unordered)",
        "category": "AI"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "hyTreeView",
          "name": "AI",
          "icon": "$(rocket)"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "hy.loginWebsite",
          "when": "viewItem == hy.notLoggedIn",
          "group": "inline"
        },
        {
          "command": "hy.loggingIn",
          "when": "viewItem == hy.notLoggedIn",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "efix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  }
}